<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Slide -->
    <sys:String x:Key="SLIDE_P2N">向上平移</sys:String>
    <sys:String x:Key="SLIDE_P2W">向左平移</sys:String>
    <sys:String x:Key="SLIDE_P2S">向下平移</sys:String>
    <sys:String x:Key="SLIDE_P2E">向右平移</sys:String>
    <sys:String x:Key="SLIDE_ZMI">放大</sys:String>
    <sys:String x:Key="SLIDE_PICK">单击缩放</sys:String>
    <sys:String x:Key="SLIDE_CTRL">拖动缩放</sys:String>
    <sys:String x:Key="SLIDE_ZMO">缩小</sys:String>

    <!-- Scale -->
    <sys:String x:Key="SCALE_M">&#8201;米</sys:String>
    <sys:String x:Key="SCALE_KM">&#8201;公里</sys:String>

</ResourceDictionary>